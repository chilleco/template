{
    "compilerOptions": {
      "target": "ES2022",
      "lib": ["DOM", "DOM.Iterable", "ES2022"],
      "allowJs": false,
      "skipLibCheck": true,
      "strict": true,
      "noImplicitAny": true,
      "module": "ESNext",
      "moduleResolution": "Node",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "isolatedModules": true,
      "noEmit": true,
      "jsx": "preserve",
      "paths": {
        "@/*": ["./*"]
      }
    },
    "include": ["next-env.d.ts", "**/*"],
    "exclude": ["node_modules"]
  }
  